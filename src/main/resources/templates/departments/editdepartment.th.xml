<thlogic>

    <attr sel="#head"
          th:replace="~{fragments/head::head('Edit Department')}" />

    <attr sel="#footerId"
          th:replace="~{fragments/footer::footer }" />

    <attr sel="#nav"
          th:replace="~{fragments/nav::nav}" />

    <attr sel="#saveDeptment"
          th:action="@{__${T(com.mpholo.learning.employees.util.DepartmentMappings).SAVE_DEPARTMENT}__}"
          th:object="${department}" />

    <attr sel="#deptNo"
          th:field="*{deptNo}"  />


    <attr sel="#deptName"
          th:field="*{deptName}" />

    <attr sel="#operation"
          th:value="${operation}" />




</thlogic>